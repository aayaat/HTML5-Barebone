{
  "name": "html5-barebone",
  "version": "1.0.0",
  "description": "A collection of HTML elements for testing UI",
  "main": "index.html",
  "scripts": {
    "jekyll:serve": "bundle exec jekyll serve",
    "live:reload": "browser-sync start --files '**/*.html, **/*.md' --files '_site/*.css' --proxy 'http://127.0.0.1:4000' --reloadDelay '2000'",
    "start": "run-p jekyll:serve live:reload"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aayaat/HTML5-Barebone.git"
  },
