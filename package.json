  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aayaat/HTML5-Barebone/issues"
  },
  "homepage": "https://github.com/aayaat/HTML5-Barebone#readme",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "npm-run-all": "^4.1.5"
  }
}
